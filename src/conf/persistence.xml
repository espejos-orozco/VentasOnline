<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.1" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="VentasOnlinePU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>app.modelo.EstatusPedidos</class>
    <class>app.modelo.PedidosDetalles</class>
    <class>app.modelo.Sucursales</class>
    <class>app.modelo.Clientes</class>
    <class>app.modelo.PedidosEncabezados</class>
    <class>app.modelo.TarjetasClientes</class>
    <class>app.modelo.Personal</class>
    <class>app.modelo.Productos</class>
    <class>app.modelo.DireccionesClientes</class>
    <class>app.modelo.VariantesProductos</class>
    <class>app.modelos.EstatusPedidos</class>
    <class>app.modelos.DireccionesClientes</class>
    <class>app.modelos.VariantesProductos</class>
    <class>app.modelos.TarjetasClientes</class>
    <class>app.modelos.Productos</class>
    <class>app.modelos.PedidosDetalles</class>
    <class>app.modelos.PedidosEncabezados</class>
    <class>app.modelos.Clientes</class>
    <class>app.modelos.Personal</class>
    <class>app.modelos.Sucursales</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost?useUnicode=true&amp;useJDBCCompliantTimezoneShift=true&amp;useLegacyDatetimeCode=false&amp;serverTimezone=UTC "/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
